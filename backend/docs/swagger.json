{
    "swagger": "2.0",
    "info": {
        "title": "BFF API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/executeScript": {
            "get": {
                "description": "executes plex2letterbox python script with current settings",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Data"
                ],
                "summary": "executes script",
                "responses": {
                    "200": {
                        "description": "Sucessfully executed export",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Settings may missing or are incomplete",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Unknown error during execution",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/exports": {
            "get": {
                "description": "returns all exisitng exports under /data",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Data"
                ],
                "summary": "returns all existing exports",
                "responses": {
                    "200": {
                        "description": "Sucessfully retreived exports",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Unknown error during execution",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/exports/{id}": {
            "get": {
                "description": "reads CSV",
                "produces": [
                    "application/octet-stream"
                ],
                "tags": [
                    "Data"
                ],
                "summary": "returns CSV of export",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Export ID (Filename)",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "CSV File",
                        "schema": {
                            "type": "file"
                        }
                    },
                    "404": {
                        "description": "File not found",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/settings": {
            "get": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Settings"
                ],
                "summary": "returns current setings",
                "responses": {
                    "200": {
                        "description": "Successfully returned settings",
                        "schema": {
                            "$ref": "#/definitions/models.Settings"
                        }
                    },
                    "400": {
                        "description": "Settings may missing or are incomplete",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Unknown error during execution",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/settings/save": {
            "post": {
                "description": "Receives baseurl and token and stores them in config.ini",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Settings"
                ],
                "summary": "saves settings",
                "parameters": [
                    {
                        "description": "Settings payload",
                        "name": "settings",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.Settings"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully saved settings",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Settings may be missing or are incomplete",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Unknown error during execution",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.Settings": {
            "type": "object",
            "properties": {
                "baseurl": {
                    "type": "string"
                },
                "token": {
                    "type": "string"
                }
            }
        }
    }
}